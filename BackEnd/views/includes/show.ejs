<% for(i=0;i< user.length; i++) { %>

<tr>
    <th scope="row"><%= i+1 %></th>
    <td><%= user[i].username %></td>
    <td><%= user[i].useremail %></td>
    <td><%= user[i].usermobile %></td>
    <td><%= user[i].usergender %></td>
    <td><%= user[i].userstatus %></td>
    <td class="px-3">
        <a href="/update_user?id=<%= user[i]._id%>"><i class="fas fa-pen-square pr-3 h5 text-success"></i></a>
       <a href="/delete_user?id=<%= user[i]._id%>"> <i class="fas fa-times text-danger  h5"></i></a>
    </td>
    
  </tr>
  <%} %>